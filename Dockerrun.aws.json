{
  "AWSEBDockerrunversion":2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "itarch/multi-client",
      "hostname": "client",
      "essential": false,
      "memory": 128
    },
    {
      "name": "server",
      "image": "itarch/multi-server",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
    {
      "name": "worker",
      "image": "itarch/multi-worker",
      "hostname": "worker",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "itarch/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappigngs": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["client", "server"],
      "memory": 128 
    }
  ]
}

